    exec "/home/carlos27/Simics/simics-6.0.128/bin/checkpoint-merge" --package-list "/home/carlos27/Desktop/proyecto-chikistrikis/src/.package-list" --project "/home/carlos27/Desktop/proyecto-chikistrikis/src" "$@"
else
    exec "/home/carlos27/Simics/simics-6.0.128/bin/checkpoint-merge" --project "/home/carlos27/Desktop/proyecto-chikistrikis/src" "$@"
fi
